{
  "expo": {
    "name": "rta_mobile_app",
    "slug": "rta_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/appicon.png",
    "scheme": "rta_mobile_app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "rta_mobile_app",
            "host": "auth"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.devkdishere.rta_mobile_app"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["rta_mobile_app"]
          }
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/appicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9db72c88-3b32-4a35-8682-777dc9797df0"
      }
    }
  }
}
