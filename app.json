{
  "expo": {
    "name": "rta_mobile_app",
    "slug": "rta_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/rtalogo.png",
    "scheme": "rta_mobile_app",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "rta_mobile_app",
            "host": "auth"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["rta_mobile_app"]
          }
        ]
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
